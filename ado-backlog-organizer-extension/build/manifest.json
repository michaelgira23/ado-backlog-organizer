{
  "manifest_version": 3,
  "name": "ADO Backlog Organizer",
  "version": "2.2",
  "description": "ADO Backlog Organizer",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webpage.js"]
    }
  ],
  "permissions": ["activeTab", "storage", "tabs", "identity", "identity.email"],
  "incognito": "not_allowed"
}
